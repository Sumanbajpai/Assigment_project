{
  "dataconnect": {
    "source": "dataconnect"
  },
  "hosting": {
    "public": "PARTY-MANAGEMENT-SYSTEM",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
